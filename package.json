{
  "name": "caprequestapp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.22.2",
    "@sap-cloud-sdk/resilience": "^3.22.2",
    "@sap/cds": "^8",
    "express": "^4",
    "@cap-js/hana": "^1"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1",
    "@sap/ux-specification": "UI5-1.130"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-requestapp": "cds watch --open requestapp/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "OP_API_PRODUCT_SRV_0001": {
        "kind": "odata-v2",
        "model": "srv/external/OP_API_PRODUCT_SRV_0001",
        "credentials": {
          "destination": "S4H",
          "path": "/sap/opu/odata/sap/API_PRODUCT_SRV"
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "sapux": [
    "app/requestapp",
    "app/requestapp"
  ]
}
